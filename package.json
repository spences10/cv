{
  "name": "cv",
  "description": "CV using Gatsby",
  "version": "1.0.619",
  "author": "Scott Spence <yo@scottspence.dev>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache/ && rm -rf public/",
    "build": "gatsby build && npm run wellknown",
    "dev": "gatsby develop -p 8080 -o",
    "format": "pretty-quick",
    "serve": "gatsby serve",
    "precommit": "pretty-quick --staged",
    "clearStale": "now rm cvjson --safe --yes",
    "wellknown": "cp -r .well-known/ public/"
  },
  "dependencies": {
    "@wesbos/dump": "0.1.1",
    "axios": "0.19.0",
    "babel-plugin-styled-components": "1.10.6",
    "date-fns": "2.8.1",
    "gatsby": "2.18.2",
    "gatsby-image": "2.2.33",
    "gatsby-node-helpers": "0.3.0",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-google-analytics": "2.1.28",
    "gatsby-plugin-manifest": "2.2.29",
    "gatsby-plugin-offline": "3.0.22",
    "gatsby-plugin-react-helmet": "3.1.15",
    "gatsby-plugin-sharp": "2.3.3",
    "gatsby-plugin-styled-components": "3.1.13",
    "gatsby-source-filesystem": "2.1.38",
    "gatsby-transformer-sharp": "2.3.5",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-seo-component": "0.1.16",
    "react-tooltip": "3.11.1",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "eslint": "6.7.1",
    "eslint-plugin-graphql": "3.1.0",
    "eslint-plugin-react": "7.16.0",
    "husky": "3.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1"
  },
  "keywords": [
    "gatsby",
    "cv"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/cv"
  },
  "bugs": {
    "url": "https://github.com/spences10/cv/issues"
  },
  "homepage": "https://github.com/spences10/cv#readme"
}
