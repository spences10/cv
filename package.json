{
  "name": "cv",
  "description": "CV using Gatsby",
  "version": "1.0.509",
  "author": "Scott Spence <spences10apps@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache/ && rm -rf public/",
    "build": "gatsby build && npm run wellknown",
    "dev": "gatsby develop --verbose --p 8080 --open",
    "format": "pretty-quick",
    "serve": "gatsby serve",
    "precommit": "pretty-quick --staged",
    "clearStale": "now rm cvjson --safe --yes",
    "wellknown": "cp -r .well-known/ public/"
  },
  "dependencies": {
    "axios": "0.19.0",
    "babel-plugin-styled-components": "1.10.6",
    "date-fns": "2.0.1",
    "gatsby": "2.14.3",
    "gatsby-image": "2.2.14",
    "gatsby-node-helpers": "0.3.0",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-google-analytics": "2.1.12",
    "gatsby-plugin-react-helmet": "3.1.5",
    "gatsby-plugin-sharp": "2.2.18",
    "gatsby-plugin-styled-components": "3.1.3",
    "gatsby-source-filesystem": "2.1.18",
    "gatsby-transformer-sharp": "2.2.12",
    "prop-types": "15.7.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1",
    "react-tooltip": "3.10.0",
    "styled-components": "4.3.2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "eslint": "6.2.2",
    "eslint-plugin-graphql": "3.0.3",
    "eslint-plugin-react": "7.14.3",
    "husky": "3.0.4",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1"
  },
  "keywords": [
    "gatsby",
    "cv"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/cv"
  },
  "bugs": {
    "url": "https://github.com/spences10/cv/issues"
  },
  "homepage": "https://github.com/spences10/cv#readme"
}
