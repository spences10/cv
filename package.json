{
  "name": "cv",
  "description": "CV using Gatsby",
  "version": "1.0.379",
  "author": "Scott Spence <spences10apps@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache/ && rm -rf public/",
    "build": "gatsby build && npm run wellknown",
    "dev": "gatsby develop --verbose --p 8080 --open",
    "format": "pretty-quick",
    "serve": "gatsby serve",
    "clearStale": "now rm cvjson --safe --yes",
    "wellknown": "cp -r .well-known/ public/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "axios": "0.19.0",
    "babel-plugin-styled-components": "1.10.1",
    "date-fns": "2.0.0-alpha.34",
    "gatsby": "2.9.9",
    "gatsby-image": "2.1.4",
    "gatsby-node-helpers": "0.3.0",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-google-analytics": "2.0.21",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sharp": "2.1.9",
    "gatsby-plugin-styled-components": "3.0.7",
    "gatsby-source-filesystem": "2.0.43",
    "gatsby-transformer-sharp": "2.1.21",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "react-tooltip": "3.10.0",
    "styled-components": "4.3.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "eslint": "5.16.0",
    "eslint-plugin-graphql": "3.0.3",
    "eslint-plugin-react": "7.13.0",
    "husky": "2.4.1",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1"
  },
  "keywords": [
    "gatsby",
    "cv"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/cv"
  },
  "bugs": {
    "url": "https://github.com/spences10/cv/issues"
  },
  "homepage": "https://github.com/spences10/cv#readme"
}
