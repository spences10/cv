  "author": "Scott Spence <spences10apps@gmail.com>",
  "dependencies": {
    "axios": "0.18.0",
    "babel-plugin-styled-components": "1.10.0",
    "date-fns": "2.0.0-alpha.27",
    "gatsby": "2.1.22",
    "gatsby-node-helpers": "0.3.0",
    "gatsby-plugin-favicon": "3.1.5",
    "gatsby-plugin-google-analytics": "2.0.16",
    "gatsby-plugin-react-helmet": "3.0.7",
    "gatsby-plugin-styled-components": "3.0.6",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-helmet": "5.2.0",
    "react-tooltip": "3.9.2",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.15.0",
    "eslint-plugin-graphql": "3.0.3",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0"
  },
  "keywords": [
    "gatsby",
    "cv"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache/ && rm -rf public/",
