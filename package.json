{
  "name": "cv",
  "description": "CV using Gatsby",
  "version": "1.0.721",
  "author": "Scott Spence <yo@scottspence.dev>",
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "dev": "gatsby develop -p 8080 -o",
    "format": "pretty-quick",
    "serve": "gatsby serve",
    "clearStale": "now rm cvjson --safe --yes"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 70,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "proseWrap": "always"
  },
  "dependencies": {
    "@wesbos/dump": "0.1.1",
    "axios": "0.19.2",
    "babel-plugin-styled-components": "1.10.7",
    "date-fns": "2.10.0",
    "gatsby": "2.19.43",
    "gatsby-image": "2.2.43",
    "gatsby-node-helpers": "0.3.0",
    "gatsby-plugin-fathom": "1.2.0",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-manifest": "2.2.47",
    "gatsby-plugin-offline": "3.0.40",
    "gatsby-plugin-react-helmet": "3.1.23",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sharp": "2.4.12",
    "gatsby-plugin-sitemap": "2.2.29",
    "gatsby-plugin-styled-components": "3.1.20",
    "gatsby-plugin-web-font-loader": "1.0.4",
    "gatsby-plugin-zeit-now": "0.2.0",
    "gatsby-source-filesystem": "2.1.55",
    "gatsby-transformer-sharp": "2.3.18",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "react-seo-component": "1.0.0",
    "react-tooltip": "4.1.2",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "husky": "4.2.3",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1"
  },
  "keywords": [
    "gatsby",
    "cv"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spences10/cv"
  },
  "bugs": {
    "url": "https://github.com/spences10/cv/issues"
  },
  "homepage": "https://github.com/spences10/cv#readme"
}
